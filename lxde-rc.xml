<keybind key="XF86AudioRaiseVolume">      <action name="Execute">        <command>amixer set Master 5%+ unmute</command>      </action>    </keybind><keybind key="XF86AudioLowerVolume">      <action name="Execute">        <command>amixer set Master 5%- unmute</command>      </action>    </keybind>
<keybind key="XF86MonBrightnessDown">      <action name="Execute">        <command>xbacklight -20</command>        <startupnotify>          <enabled>yes</enabled>          <name>Decrease screen brightness</name>        </startupnotify>      </action>    </keybind>
<keybind key="XF86MonBrightnessUp">      <action name="Execute">        <command>xbacklight +20</command>        <startupnotify>          <enabled>yes</enabled>          <name>Increase screen brightness</name>        </startupnotify>      </action>    </keybind>
